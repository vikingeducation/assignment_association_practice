	  <%= javascript_include_tag 'application' %>
	  <%= csrf_meta_tags %>

	  <!-- Meta Stuff -->
	  <meta charset="utf-8">

	  <!-- Bootstrap CSS -->
	  <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.2.0/css/bootstrap.min.css">

	  <!-- jQuery -->
